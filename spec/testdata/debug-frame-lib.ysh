const __provide__ = :| print-stack my-proc |

proc print-stack {
  for i, fr in (vm.getDebugStack()) {
    write --end '' -- $[formatDebugFrame(fr, prefix="  #$[i+1] ")]
    #write -- $[formatDebugFrame(fr, i+1)]
  }
}

proc my-proc {
  print-stack
}

proc call-func {
  var x = myfunc(99)
  echo $x
}

func myfunc2(a, b) {
  print-stack
  return (a + b)
}

func myfunc(x) {
  return (myfunc2(x+1, 42))
}
